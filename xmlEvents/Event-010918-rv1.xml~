      </fn:image>
        <fn:location>
          <fn:oid>6507658</fn:oid>
          <fn:name>Honda Center</fn:name>
          <fn:address>2695 E. Katella Ave.</fn:address>
          <fn:address_supplement></fn:address_supplement>
          <fn:city>Anaheim</fn:city>
          <fn:state>CA</fn:state>
