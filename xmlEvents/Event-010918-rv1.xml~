          <fn:special_placement>Web Only</fn:special_placement>
          <fn:feature>Image</fn:feature>
        <fn:image>
        <fn:url>https://images1.ocweekly.com/imager/u/original/8564180/5e6410ca-48ca-4802-be54-f60fa312a421_42061_tablet_landscape_large_16_9.jpg</fn:url>
