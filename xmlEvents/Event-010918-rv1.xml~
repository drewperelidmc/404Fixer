        <fn:location>
          <fn:oid>6504643</fn:oid>
          <fn:name>The Swallow&#39;s Inn</fn:name>
          <fn:address>31786 Camino Capistrano</fn:address>
          <fn:address_supplement></fn:address_supplement>
          <fn:city>San Juan Capistrano</fn:city>
          <fn:state>CA</fn:state>
          <fn:postal_code>92675</fn:postal_code>
          <fn:neighborhood_group>OC - South</fn:neighborhood_group>
          <fn:neighborhood>San Juan Capistrano</fn:neighborhood>
