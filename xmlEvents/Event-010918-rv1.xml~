        <title>The White Buffalo</title>
        <fn:title>The White Buffalo</fn:title>
        <link>/oc/the-white-buffalo/Event?oid=8352360</link>
        <description></description>
        <fn:description></fn:description>
        <fn:price>TBA</fn:price>
        <fn:phone></fn:phone>
