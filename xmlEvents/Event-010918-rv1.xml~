          <fn:oid>6495368</fn:oid>
          <fn:name>Hamburger Mary&#39;s</fn:name>
          <fn:address>330 Pine Ave.</fn:address>
          <fn:address_supplement></fn:address_supplement>
          <fn:city>Long Beach</fn:city>
