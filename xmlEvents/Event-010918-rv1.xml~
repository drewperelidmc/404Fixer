          <fn:special_placement>Web Only</fn:special_placement>
          <fn:feature>Image</fn:feature>
        <fn:image>
        <fn:url>https://images1.ocweekly.com/imager/u/original/8589972/fa9caa1f-73a1-411e-b507-ec56fa59650a_106061_tablet_landscape_large_16_9.jpg</fn:url>
