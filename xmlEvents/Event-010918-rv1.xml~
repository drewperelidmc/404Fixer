          <fn:special_placement>Web Only</fn:special_placement>
          <fn:feature>Image</fn:feature>
        <fn:image>
        <fn:url>https://images1.ocweekly.com/imager/u/original/8641521/b12a71b0-05a2-401b-8ca7-bfbb686451ab_110201_tablet_landscape_large_16_9.jpg</fn:url>
