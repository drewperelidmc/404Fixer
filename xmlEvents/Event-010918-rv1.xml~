        <fn:title>Big Boi with the Cool Kids</fn:title>
        <link>/oc/big-boi-with-the-cool-kids/Event?oid=8611794</link>
        <description></description>
        <fn:description></fn:description>
          <fn:artist>Big Boi</fn:artist>
          <fn:artist>The Cool Kids</fn:artist>
        <fn:price>TBA</fn:price>
        <fn:phone></fn:phone>
        <fn:url></fn:url>
          <fn:event_category>
            <fn:section_name>Music</fn:section_name>
