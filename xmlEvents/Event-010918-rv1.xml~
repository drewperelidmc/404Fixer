          <fn:artist>Yngwie Malmsteen</fn:artist>
        <fn:price>TBA</fn:price>
        <fn:phone></fn:phone>
        <fn:url></fn:url>
          <fn:event_category>
            <fn:section_name>Music</fn:section_name>
